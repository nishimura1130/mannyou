
<%= form_with(model: [:admin, @user], local: true) do |f| %>
<div class="user_title">
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :email %>
  <%= f.email_field :email %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>
  <%= f.label :admin %>
  <%= f.check_box :admin %>
</div>
<%= f.submit "実行"%>
<%= link_to '管理者画面/ユーザー一覧へ', admin_users_path, class: "btn btn-dark btn-sm" %>
<% end %>